#include <stdio.h>
#include <stdlib.h>
#include<stdarg.h>

int suma(int n,...)
{
    int i,s=0;
    va_list lista;
    va_start(lista,n);
    
    for(i=0;i<n;i++)
    {
        x=va_arg(lista,int);
        s=s+x;
    }
    return s;
}

int main()
{
    int x;
    x=suma(4,1,2,3,4);
    printf("%")
    return 0;
}
