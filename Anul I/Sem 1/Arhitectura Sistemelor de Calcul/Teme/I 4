.data
y: .space 4
v: .word 1 4 3 1 7 5
n: .word 6
x: .word 4

.text
main:

lw $t0,n # n=nr de elemente
mul $t0,$t0,4

lw $t2,x # elementul de gasit
li $t4,0
sw $zero,y
for:

bge  $t4,$t0 iesire # i>=n iesire

lw $t5,v($t4)

beq $t5,$t2 gasit
addi $t4,$t4,4

j for

gasit:
li $t6,1
sw $t6,y

iesire:
lw $a0,y
li $v0,1
syscall

li $v0,10
syscall
