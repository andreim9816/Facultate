.data
v: .word 2 -3 4 1 -1 5 7 8 9 0
mesaj: .asciiz "Program incheiat"

.text
main:

subu $sp,$sp,4
la $t0,v
sw $t0,0($sp)

jal functie

li $v0,4
la $a0,mesaj
syscall

li $v0,10
syscall 

functie:

subu $sp,$sp,4
sw $fp,0($sp)
addiu $fp,$sp,0

li $t0,0 # numarul de elemente diferite de 0
lw $t1,4($fp) #adresa
lw $t2,0($t1) # element
for:

beqz $t2, iesire
addi $t0,$t0,1
addi $t1,$t1,4
lw $t2,0($t1)

j for

iesire:
sw $t0,0($t1)

lw $fp,0($fp)
addiu $sp,$sp,4

jr $ra



