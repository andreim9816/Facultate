.data
sir: .asciiz "muiePasede"
c: .byte 'e'
n: .word 5
.text
div 
main:
subu $sp,$sp,12
la $t0,sir
sw $t0,0($sp)
lb $t0,c
sw $t0,4($sp)
lw $t0,n
sw $t0,8($sp)

jal functie

addi $sp,$sp,12

li $v0,4
la $a0,sir
syscall 

li $v0,10
syscall

functie:

subu $sp,$sp,4
sw $fp,0($sp)
addiu $fp,$sp,0

lb $t0,8($fp) # litera c
lw $t1,4($fp) #adresa de inceput
lw $t2,12($fp) # n
add $t1,$t1,$t2 #a n-a litera
sb $t0,0($t1)

lw $fp,0($sp)
addi $sp,$sp,4

jr $ra